<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Involio" parent="Theme.AppCompat.DayNight.DarkActionBar">
        <item name="android:forceDarkAllowed">false</item>
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/lightBar_color</item>
        <item name="colorPrimaryVariant">@color/darkBar_color</item>
        <item name="colorOnPrimary">@color/text_color</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">@color/backrgound_color</item>

        <!-- Customize your theme here. -->
    </style>
</resources>